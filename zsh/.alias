#!/bin/zsh

alias glgg="git log --topo-order --all --graph --oneline"
alias glgb="git log --topo-order --pretty=short"
alias glgd="git log --topo-order --patch"

alias iphone-simulator="open /Applications/Xcode.app/Contents/Developer/Applications/Simulator.app"

alias ls=eza
alias l="eza --oneline --all"

alias dc="docker compose"

alias rm="safe-rm"
alias v="nvim"
alias vim=nvim

alias unsafe-rm="command rm"

alias ssh="TERM=xterm-256color ssh"

source ~/.alias_secret
